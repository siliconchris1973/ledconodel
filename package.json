{
  "name": "ledconodel",
  "description": "A tiny Node.JS app to watch an adafruit.io feed and turn on/off 3 leds connected to the Raspberry PI",
  "version": "0.1.0",
  "author": "c.guenther@mac.com",
  "license": "ISC",
  "private": true,
  "keywords": [
    "Adafruit.IO",
    "Node.JS",
    "Raspberry PI",
    "IoT"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siliconchris1973/ledconodel.git"
  },
  "scripts": {
    "start": "sudo node ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall" : "node -e \"var s='../src',d='node_modules/src',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\""
  },
  "homepage": "https://github.com/siliconchris1973/ledconodel#readme",
  "bugs": {
    "url": "https://github.com/siliconchris1973/ledconodel/issues"
  },
  "config": {
    "port": "9000"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "express": "latest",
    "pug": "~2.0.0-beta11",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2",
    "wiring-pi": "~2.2.1",
    "debug": "latest"
  }
}
